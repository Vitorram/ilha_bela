generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Casa {
  id      Int    @id @default(autoincrement())
  link    String
  valor   Float
  usuario String
}

model ItemChurrasco {
  id    Int    @id @default(autoincrement())
  nome  String
  valor Float
}
